module whiley.WhileyI

header {
    import whiley.WhileyParser.*;
}

Program =
  Spacing Stm* EOF.

Stm {line} =
		Type Loc								{Decl}.

Type = 
    "int"   								{IntType}
    "byte"  								{ByteType}
    "bool"  								{BoolType}.

Digits : String = 
		Digit*

Digit : String =
		[0-9].

Loc = 
    Identifier.

Lab : String =
		Identifier.